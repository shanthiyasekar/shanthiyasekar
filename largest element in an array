#include <iostream>

using namespace std;

int main()
{
    
    int arr[]={10,5,20,8};
    int n=sizeof(arr)/sizeof(arr[0]);
    int flag=0;
    int i;
    for( i=0;i<n;i++){
        flag=0;
        for(int j=0;j<n;j++){
            if(arr[j]>arr[i]){
                flag=1;
                break;
            
            }
        }
        if(flag==0){
            cout<<i;
            break;
        }
    }
    
    
}
    
