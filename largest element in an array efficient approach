#include <bits/stdc++.h>

using namespace std;

int main()
{
    
    int arr[]={10,5,20,8};
    int n=sizeof(arr)/sizeof(arr[0]);
    int max=INT_MIN;
    int res=0;
    for(int i=0;i<n;i++){
        if(arr[i]>max){
            max=arr[i];
            res=i;
        }
    }
    cout<<max<<" "<<res;
    
}
    
